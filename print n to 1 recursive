public class NumberPrinter {
    public static void printNumbers(int n) {
        if (n >= 1) {
            System.out.println(n);
            printNumbers(n - 1);
        }
    }
    
    public static void main(String[] args) {
        int n = 10; // Replace with the desired value of n
        printNumbers(n);
    }
}

TC O(n) 
